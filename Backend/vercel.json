{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/cron.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/cron", "dest": "api/cron.js" },
    { "src": "/(.*)", "dest": "api/index.js" }
  ],
  "crons": [
    { "path": "/api/cron", "schedule": "*/5 * * * *" }
  ]
}
